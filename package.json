{
  "name": "@specbeelabs/generator-drupal-theme-init",
  "version": "1.5.1",
  "description": "A Yeoman generator to scaffold and initialize a Drupal theme.",
  "homepage": "https://github.com/SpecbeeLabs/drupal-theme-init",
  "author": "Sagar <sagar@specbee.com> (https://specbee.com)",
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator",
    "drupal theme",
    "drupal 8",
    "drupal 9",
    "drupal 10"
  ],
  "devDependencies": {
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^7.4.0"
  },
  "engines": {
    "npm": ">= 8.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "yeoman-generator": "^5.8.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SpecbeeLabs/drupal-theme-init.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SpecbeeLabs/drupal-theme-init/issues"
  },
  "directories": {
    "doc": "docs"
  }
}
